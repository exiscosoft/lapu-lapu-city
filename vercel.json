{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_GOVERNMENT_NAME": "@vite_government_name",
    "VITE_GOVERNMENT_TYPE": "@vite_government_type",
    "VITE_REGION": "@vite_region",
    "VITE_PROVINCE": "@vite_province",
    "VITE_WEBSITE_URL": "@vite_website_url",
    "VITE_CONTACT_EMAIL": "@vite_contact_email",
    "VITE_CONTACT_PHONE": "@vite_contact_phone"
  }
}
